	issn = {1539-2791, 1559-0089},
	journal = {Neuroinformatics},
	language = {en},
	month = jan,
	pmid = {25631538},
	title = {Automatic Morphological Reconstruction of Neurons from Multiphoton and Confocal Microscopy Images Using {3D} Tubular Models},
